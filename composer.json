{
  "name": "bartoszbartniczak/symfony-serializer-arrayobject-normalizer",
  "description": "ArrayObject Normalizer for Symfony/Serializer component. This Normalizer works with ArrayObject objects and its subclasses.",
  "minimum-stability": "stable",
  "license": "Apache 2.0",
  "authors": [
    {
      "name": "Bartosz Bartniczak",
      "email": "kontakt@bartoszbartniczak.pl"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/serializer": "^3.0|^4.0|^5.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "symfony/property-access": "~2.8|~3.0",
    "satooshi/php-coveralls": "*"
  },
  "autoload": {
    "psr-4": {
      "BartoszBartniczak\\SymfonySerializer\\Normalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BartoszBartniczak\\SymfonySerializer\\Normalizer\\": ["tests/unit-tests/", "tests/integration-tests/"]
    }
  }
}
